{
  "name": "graphql_what_why",
  "version": "1.0.0",
  "repository": "https://github.com/PabloSzx/graphql_what_why",
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "main": "index.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development run-p -l dev-*",
    "dev-client": "ts-node-dev --respawn ./client.ts",
    "dev-graphql": "cross-env PORT=3001 ts-node-dev --respawn ./graphql/index.ts",
    "dev-rest": "cross-env PORT=3002 ts-node-dev --respawn ./rest/index.ts",
    "dev-typegraphql": "cross-env PORT=3003 ts-node-dev --respawn ./type-graphql/index.ts",
    "start": "cross-env NODE_ENV=production run-p -l start-*",
    "start-client": "ts-node ./client.ts",
    "start-graphql": "cross-env PORT=3001 ts-node ./graphql/index.ts",
    "start-rest": "cross-env PORT=3002 ts-node ./rest/index.ts",
    "start-typegraphql": "cross-env PORT=3003 ts-node ./type-graphql/index.ts"
  },
  "dependencies": {
    "apollo-server": "^2.9.16",
    "apollo-server-express": "^2.9.16",
    "axios": "^0.19.2",
    "chokidar": "^3.3.1",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "graphql-request": "^1.8.2",
    "graphql-tag": "^2.10.1",
    "graphql-tag-ts": "^0.1.14",
    "lodash": "^4.17.15",
    "node-notifier": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "require-env-variable": "^3.0.1",
    "type-graphql": "^0.17.6",
    "wait-on": "^4.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.0",
    "@types/node-notifier": "^5.4.0",
    "@types/wait-on": "^3.2.0",
    "cross-env": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.0-beta"
  },
  "resolutions": {
    "@types/express": "4.17.2",
    "@types/express-serve-static-core": "4.17.0"
  }
}
